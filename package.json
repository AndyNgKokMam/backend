{
  "name": "backend-coding-test",
  "version": "1.0.0",
  "description": "A coding assessment for Xendit backend interviews",
  "main": "index.js",
  "engines": {
    "node": ">8.6 <= 10"
  },
  "scripts": {
    "test:lint": "./node_modules/.bin/eslint .",
    "test:lint-fix": "./node_modules/.bin/eslint . --fix",
    "test:unit": "./node_modules/.bin/nyc ./node_modules/.bin/mocha --config .mocharc.json test/**/*.test.ts",
    "test": "npm run test:lint && npm run test:unit && ./node_modules/.bin/nyc report --reporter=html",
    "test:watch": "./node_modules/.bin/nodemon -x 'npm test'",
    "test:artillery-debug": "SET DEBUG=http:response && artillery run test-load/load-test.yml",
    "test:artillery": "./node_modules/.bin/artillery run test-load/load-test.yml report --output reports/report.html reports/report.json",
    "test:load": "start-server-and-test start http://localhost:8010/health test:artillery",
    "lint": "eslint index.ts ./**/*.{js,ts}",
    "report": "./node_modules/.bin/nyc report --reporter=html && ./node_modules/.bin/opn coverage/index.html",
    "start-test": "node index.ts",
    "start": "nodemon index.ts",
    "serve1": "pm2 delete all && pm2 start ecosystem.config.js --no-daemon",
    "serve": "pm2 start ecosystem.config.js --no-daemon",
    "clean": "pm2 delete all",
    "build": "npm run clean && SET NODE_ENV=production && npx webpack --config webpack.config.js --node-env production",
    "gentoc": "markdown-toc -i README.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,json,ts}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,ts}": [
      "eslint",
      "git add"
    ],
    "README.md": [
      "markdown-toc -i",
      "git add"
    ]
  },
  "keywords": [
    "Xendit"
  ],
  "author": "Andy Ng Kok Mam, Bo Chen",
  "license": "MIT",
  "dependencies": {
    "ajv": "8.6.3",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-openapi-validator": "^4.13.1",
    "helmet": "^4.6.0",
    "mongodb": "4.1.2",
    "morgan": "1.10.0",
    "sequelize": "^6.6.5",
    "sqlite3": "5.0.2",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@tsconfig/node10": "1.0.8",
    "@types/artillery": "1.6.2",
    "@types/body-parser": "1.19.1",
    "@types/chai": "4.2.21",
    "@types/compression": "^1.7.2",
    "@types/cors": "2.8.12",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.13",
    "@types/mocha": "9.0.0",
    "@types/mongodb": "^4.0.7",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.9.1",
    "@types/sinon": "10.0.2",
    "@types/sqlite3": "3.1.7",
    "@types/supertest": "2.0.11",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "artillery": "1.7.9",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-polyfill": "6.26.0",
    "chai": "4.3.4",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "14.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "markdown-toc": "1.2.0",
    "mocha": "9.1.1",
    "nodemon": "2.0.12",
    "nyc": "15.1.0",
    "opn-cli": "5.0.0",
    "pm2": "^5.1.1",
    "prettier": "2.4.1",
    "sinon": "11.1.2",
    "supertest": "6.1.6",
    "ts-loader": "^9.2.5",
    "ts-node": "10.2.1",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "3.11.0",
    "tscpaths": "0.0.9",
    "tslint": "6.1.3",
    "typescript": "^4.4.3",
    "typescript-memoize": "1.0.1",
    "typescript-transform-paths": "3.3.1",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
